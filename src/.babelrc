{
  "presets": [
    [
      "react-app",
      {
        "runtime": "automatic"
      }
    ],
    "mobx",
    "@babel/react",
    "@babel/env"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 2,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ],
    "@babel/plugin-proposal-class-properties",
    "react-hot-loader/babel"
  ]
}